<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <!--PrivateAssets 属性指定哪些资产（如编译符号、内容文件等）不会传递给引用当前项目的其他项目-->
    <!--ExcludeAssets 属性指定哪些资产类型不会被包含在项目的最终输出（如发布包）中-->
    <PackageReference Include="AutoCtor" Version="2.8.0" PrivateAssets="all" ExcludeAssets="runtime" />
    <PackageReference Include="Injectio" Version="5.0.0" PrivateAssets="all" />
    <PackageReference Include="NetEscapades.EnumGenerators" Version="1.0.0-beta12" PrivateAssets="all" ExcludeAssets="runtime" />
    <PackageReference Include="Riok.Mapperly" Version="4.2.1" ExcludeAssets="runtime" PrivateAssets="all" />
  </ItemGroup>

</Project>
